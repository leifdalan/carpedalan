    "superagent": "^4.1.0",
    "swagger-js": "^1.0.0",
    "swagger-ui-express": "^4.0.2",
    "up": "^1.0.2",
    "uuid": "^3.3.2",
    "webpack": "^4.29.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-s3-plugin": "^1.0.3",
    "winston": "^3.2.1",
    "winston-loggly-bulk": "https://github.com/loggly/winston-loggly-bulk.git#3.x"
  },
  "optionalDependencies": {
    "cypress": "3.2.0"
  }
}
