{
  "name": "carpedalan",
  "version": "2.0.0",
  "description": "Carpe Dalan",
  "main": "index.js",
  "scripts": {
    "test": "LOG_LEVEL=none jest",
    "lint": "eslint src api-v1 db imageResizer scripts server shared  --ext .js,.jsx",
    "integration": "NODE_ENV=test PG_HOST=localhost PG_PORT=5555 jest -c jestrc.js --runInBand",
    "integration:ci": "PG_HOST=localhost NODE_ENV=ci jest -c jestrc.js --runInBand --verbose",
    "build": "BABEL_ENV=production NODE_ENV=production yarn webpack --mode production --config webpack.prod.js",
    "migrateAndSeed": "MIGRATING=true ./scripts/migrateAndSeed.sh",
    "migrate": "MIGRATING=true ./scripts/migrate.sh",
    "migrateAndStart": "MIGRATING=true yarn migrateAndSeed && DEBUG=knex*,-knex:client,-knex:connection,-knex:pool nodemon --inspect=0.0.0.0 .",
    "start": "DEBUG=knex*,-knex:client,-knex:connection,-knex:pool nodemon --inspect=0.0.0.0 .",
    "start:prod": "NODE_ENV=production node .",
    "u": "docker-compose up",
    "d": "docker-compose down",
    "du": "docker-compose down && docker-compose up",
    "dub": "docker-compose down && docker-compose up --build",
    "e2e": "PG_PORT=5433 PG_HOST=localhost CYPRESS_BASE_URL=http://local.carpedalan.com cypress open --env configFile=local",
    "cypress": "PG_PORT=5555 PG_HOST=localhost CYPRESS_BASE_URL=http://local.carpedalan.com cypress run --env configFile=local",
    "e2e:ci": "MIGRATING=true NODE_ENV=ci yarn migrate NODE_ENV=ci yarn knex seed:run --knexfile db/knexfile.js && yarn start:prod",
    "test-junit": "cypress run --reporter junit --reporter-options 'mochaFile=junit-results/my-test-output.xml'",
    "test-multiple": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=config.json",
    "updateConfig": "aws s3 cp .env-prod s3://configurations-pipeline/.env --profile carpedev",
    "generateCerts": "sudo openssl req -x509 -nodes -days 730 -newkey rsa:2048 -keyout ./nginx/nginx-selfsigned.key -out ./nginx/nginx-selfsigned.crt -config ./nginx/req.cnf -sha256"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 38.5,
        "branches": 10.5,
        "functions": 15.5,
        "lines": 20
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-virtualized|react-select|)/)"
    ],
    "roots": [
      "src"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-preset-jest": "^24.1.0",
    "codecov": "^3.3.0",
    "cypress-dark": "^1.7.11",
    "cypress-file-upload": "^3.0.7",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "fs-extra": "^7.0.1",
    "glob": "7.1.3",
    "happypack": "^5.0.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-junit": "^6.2.1",
    "jest-styled-components": "^6.3.1",
    "lighthouse": "^4.1.0",
    "lint-staged": "^8.1.3",
    "openapi-response-validator": "^3.7.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "react-test-renderer": "16.8.0",
    "supertest": "^3.4.2",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-stylish": "^0.1.8"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@sentry/browser": "^4.5.3",
    "@sentry/node": "^4.5.3",
    "aws-cloudfront-sign": "^2.2.0",
    "aws-sdk": "^2.397.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^1.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "date-fns": "2.0.0-alpha.26",
    "docker-compose": "^0.10.0",
    "dotenv-safe": "^6.1.0",
    "eslint-plugin-react-hooks": "^1.0.0",
    "esm": "^3.2.1",
    "exif-parser": "^0.1.12",
    "exifreader": "^2.7.0",
    "express": "^4.16.4",
    "express-cache-controller": "^1.1.0",
    "express-handlebars": "^3.0.0",
    "express-openapi": "^4.4.0",
    "express-session": "^1.15.6",
    "express-winston": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "logrocket": "^0.6.19",
    "moment": "^2.24.0",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react": "16.8.0",
    "react-day-picker": "^7.2.4",
    "react-dom": "16.8.0",
    "react-hot-loader": "^4.6.5",
    "react-loadable": "^5.5.0",
    "react-router": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "react-select": "^2.3.0",
    "react-virtualized": "^9.21.0",
